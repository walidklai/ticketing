{
  "extends": ["plugin:react/recommended", "google", "prettier"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "@typescript-eslint", "prettier"],
  "rules": {
    "new-cap": ["warn", { "newIsCap": true, "capIsNew": false }],
    "no-console": "off",
    "require-jsdoc": "off",
    "no-invalid-this": "warn"
  }
}
